<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zirkadashboard">

    <application
        android:allowBackup="true"
        android:icon="@drawable/zirka"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Users"
            android:theme="@style/AppTheme.NoTitle"></activity>
        <activity android:name=".EventDetails"
            android:theme="@style/AppTheme.NoTitle"/>
        <activity android:name=".Events"
            android:theme="@style/AppTheme.NoTitle"/>
        <activity
            android:name=".Dashboard"
            android:theme="@style/AppTheme.NoTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Earnings"
            android:theme="@style/AppTheme.NoTitle"/>
    </application>

</manifest>